{
  "name": "recipe-hunter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "submit-android": "eas submit --platform android",
    "submit-ios": "eas submit --platform ios",
    "auto-submit": "eas build --platform android --auto-submit",
    "auto-submit-ios": "eas build --platform ios --profile production --auto-submit",
    "dapp-store": "dotenv -e .env -- eas build --local -p android --profile dapp-store",
    "build:test": "eas build  -p android --profile test",
    "build:test-local": "dotenv -e .env -- eas build --local -p android --profile test",
    "build:iostest": "npx expo run:ios",
    "build:ios": "eas build --platform ios --profile production",
    "build:ios-local": "dotenv -e .env -- eas build --local -p ios --profile production",
    "build:android": "eas build -p android --profile production",
    "build:android-local": "node ./scripts/version-bump.js",
    "build:android-local-original": "dotenv -e .env -- eas build --local -p android --profile production",
    "test-version-bump": "node ./scripts/test-version-bump.js",
    "apk-type": "dotenv -e .env -- eas build --local -p android --profile apk --clear-cache",
    "logs": "./scripts/check-logs.sh",
    "logs:errors": "./scripts/check-logs.sh errors",
    "logs:react": "./scripts/check-logs.sh react"
  },
  "dependencies": {
    "@expo-google-fonts/poppins": "^0.4.1",
    "@expo-google-fonts/space-grotesk": "^0.4.1",
    "@expo/vector-icons": "^15.0.3",
    "@gorhom/bottom-sheet": "^5.2.8",
    "@react-native-async-storage/async-storage": "^2.1.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@shopify/react-native-skia": "^2.4.14",
    "axios": "^1.6.5",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "expo": "~54.0.30",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.12",
    "expo-dev-client": "~6.0.20",
    "expo-document-picker": "~13.0.3",
    "expo-file-system": "~18.0.10",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linking": "~8.0.11",
    "expo-local-authentication": "^17.0.8",
    "expo-notifications": "^0.32.15",
    "expo-router": "~6.0.21",
    "expo-secure-store": "^15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "~29.0.15",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-feather": "^1.1.2",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-purchases": "^9.6.13",
    "react-native-purchases-ui": "^9.6.14",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.19",
    "uuid": "^13.0.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "dotenv": "^17.2.4",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
